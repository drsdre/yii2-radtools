{
    "name": "drsdre/yii2-radtools",
    "description": "Rapid Application Development tools to quickly build interconnected crud UI. Uses yii2-ajaxcrud to generate the pop-up forms and (optionally) kartik-v's Dynagrid.",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","rad","crud","ajaxcrud","ajax","hierarchy"],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "A.F.Schuurman",
            "email": "andre.schuurman_yii2radtools@gmail.com"
        }
    ],
    "repositories":[
        {
            "type": "vcs",
            "url": "https://github.com/drsdre/yii2-ajaxcrud"
        },
    ],
    "require": {
        "php": ">=7.0.0",
        "yiisoft/yii2": "*",
        "johnitvn/yii2-ajaxcrud": "dev-master",
        "kartik-v/yii2-dynagrid": "~1.4"
    },
    "autoload": {
        "psr-4": {
            "drsdre\\radtools\\": ""
        }
    }
}
